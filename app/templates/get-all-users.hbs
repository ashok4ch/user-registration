<form action="" class="register">
<h1>Get All User</h1>
    <table class="table1">
   {{#if model}}
      <th>First Name </th>
      <th>Middle Name</th>
      <th>Last Name </th>
      <th>User Edit </th>

        {{#each model as |user|}}

            <tr>
                <td>{{user.firstName}}</td>
                <td>{{user.middleName}}</td>
                <td>{{user.lastName}}</td>
                <td>{{link-to 'Details' 'get-user' user.userId}}</td>
            </tr>
        {{/each}}
           {{else}}
             No record found.
            {{/if}}
    </table>
    <br/> <br/>
{{link-to 'Back Add User' 'userform'}}
</form>